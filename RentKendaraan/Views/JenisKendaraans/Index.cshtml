@model IEnumerable<RentKendaraan.Models.JenisKendaraan>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NamaJenisKendaraan)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NamaJenisKendaraan)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdJenisKendaraan"><i class="fa fa-marker fa-lag"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdJenisKendaraan"><i class="fas fa-list"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdJenisKendaraan"><i class="fa fa-trash-alt fa-lag"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
